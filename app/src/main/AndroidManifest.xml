<?xml version="1.0" encoding="utf-8"?>
<manifest package="osu.cse.xuan.freqdetector"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MessengerMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Activities.Pop"/>
        <activity
            android:name=".Activities.FrequencyTracker"
            android:label="@string/title_activity_messenger_main"
            android:theme="@style/AppTheme.NoActionBar"/>
        <!--
             ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>

        <activity
            android:name=".Activities.ReceivedMessages"
            android:label="@string/title_activity_recieved_messages"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".Activities.SentMessages"
            android:label="@string/title_activity_sent_messages"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".Activities.GenerateKey">
        </activity>
        <activity android:name=".retrievedKeys"></activity>
    </application>

</manifest>
